{
  "name": "roamjs-static-site",
  "version": "1.0.1",
  "description": "AWS Lambda Function generating and deploying Roam graphs as public sites",
  "main": "out/deploy.js",
  "scripts": {
    "action": "node scripts/buildActions.js",
    "test": "jest --config jestconfig.json",
    "prebuild": "rm -Rf out",
    "lambdas": "cross-env NODE_ENV=production roamjs-scripts lambdas",
    "build": "roamjs-scripts build",
    "build:components": "node scripts/buildComponents.js",
    "precomponents": "npm run build:components",
    "components": "http-server -p 3007 build",
    "deploy": "roamjs-scripts deploy",
    "dev": "roamjs-scripts dev",
    "preserver": "cross-env NODE_ENV=development roamjs-scripts lambdas --build",
    "server": "localhost-lambdas --port 3005 --tunnel vargas",
    "start": "concurrently npm:dev npm:server npm:components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvargas92495/roamjs-static-site.git"
  },
  "author": "dvargas92495 <dvargas92495@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvargas92495/roamjs-static-site/issues"
  },
  "homepage": "https://github.com/dvargas92495/roamjs-static-site#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@blueprintjs/core": "^3.44.3",
    "aws-sdk": "^2.826.0",
    "axios": "^0.21.4",
    "codemirror": "^5.61.1",
    "cytoscape": "^3.19.1",
    "jsdom": "^16.6.0",
    "mime-types": "^2.1.32",
    "mustache": "^4.2.0",
    "namor": "^2.0.2",
    "puppeteer": "^5.4.0",
    "puppeteer-core": "^5.5.0",
    "randomstring": "^1.2.1",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "roamjs-components": "^0.42.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.70",
    "@types/cytoscape": "^3.14.17",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/mime-types": "^2.1.1",
    "@types/mustache": "^4.1.2",
    "@types/puppeteer": "^5.4.4",
    "@types/puppeteer-core": "^5.4.0",
    "@types/randomstring": "^1.1.7",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/url-regex-safe": "^1.0.0",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "esbuild-jest": "^0.5.0",
    "http-server": "^13.0.2",
    "jest": "^27.3.1",
    "localhost-lambdas": "^0.5.5",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "roamjs-scripts": "^0.13.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.1.2"
  }
}
